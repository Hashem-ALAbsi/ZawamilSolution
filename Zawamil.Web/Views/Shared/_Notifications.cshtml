@if (TempData["info"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js" integrity="sha512-lbwH47l/tPXJYG9AcFNoJaTMhGvYWhVM9YI43CT+uteTRRaiLCui8snIgyAN8XWgNjNhCqlAUdzZptso6OCoFQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script type="text/javascript">
        toastr.success('@TempData["info"]')
        
    </script>
    <div>
        <script>
                Swal.fire({
                    title: 'Do You want this ?',
                    icon: 'question',
                    showCancelButton: true,
                }).then(result => {
                    if (result.isConfirmed) {
                        TempData["question"] = "question";
                        return TempData["question"]; 
                        // إعادة توجيه المستخدم إلى رابط الفعل
                       // window.location.href = '/User/' + url + '?id=' + userId; // استبدل YourController باسم التحكم الفعلي
                    }
                });
            
        </script>
    </div>

        
    
    
       @*  <div class="alert alert-success">
            <strong>Success !</strong>@TempData["success"]
        </div> *@
}

